    name : BTN_Envoyer
    identifier : 0x284d317da0e0352c
    internal_properties : 
    properties :
     x : 548
     y : 572
     width : 118
     height : 34
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        internal_properties : 
        code : |1+
         
         //Vérification que l'on ait bien un destinataire à qui envoyer le mail
         SI SAI_Destinataires = "" ALORS
         	
         	Erreur("Vous devez saisir un destinataire.")
         	RepriseSaisie(SAI_Destinataires)
         	
         SINON
         	
         	Sablier(Vrai)
         	
         	//Remise à 0 de tous les membres de la structure Email
         	emailraz()
         	
         	//Initialisation des options du mail : confirmation de lecture, priorité...
         	InitOptionsMail()
         	
         	//Informations principales de l'email ...
         	Email.Expéditeur = gsEmailExpediteur
         	Email.Sujet = SAI_SUJET
         	//La fonction WLangage "RTFVersHTML" permet de transformer en 1 ligne de code du RTF en HTML
         	Email.HTML = EDHTML_email
         	
         	
         	//Initialisation des différents types de destinataires
         	InitDestinataires(SAI_Destinataires,1)
         	InitDestinataires(SAI_CC,2)
         	InitDestinataires(SAI_CCI,3)
         	
         	//Initialisation des pièces jointes
         	InitPJ()
         	
         	EmailChangeTimeOut(10)
         	//Ouverture de la connexion SMTP
         	SI PAS EmailOuvreSessionSMTP(gsNomUtilisateur,gsPassWord,gsServeurSMTP) ALORS
         		
         		Erreur("Impossible d'établir la connexion avec le serveur.","Veuillez réessayer plus tard ou vérifier vos paramètres.")
         		
         	SINON
         		
         
         <SI CibleExécution=ApplicationWindows>
         	
         		// Ouvre la fenêtre de jauge
         		ouvrefille(fen_jauge)
         		//Jauge de visualisation de progression de l'envoi de mail
         		EmailJauge(FEN_Jauge.JAUGE_EnvoiEmail)
         	
         <FIN>
         
         	
         		//Envoi du message
         		SI PAS EmailEnvoieMessage(gsNomUtilisateur) ALORS
         			Sablier(Faux)
         			Erreur("Cet e-mail n'a pas pu être envoyé. Veuillez vérifier votre paramétrage de compte email.")
         			RepriseSaisie(mafenêtre)
         		SINON
         			Sablier(Faux)
         			info("L'email a bien été envoyé.")
         		FIN
         		
         
         <SI CibleExécution=ApplicationWindows>
         	
         			//Arrêt de la jauge
         			EmailJauge("")
         	
         <FIN>
         
         
         		// Fermeture de la session SMTP
         		EmailFermeSession(gsNomUtilisateur)
         		
         	FIN
         	
         	Sablier(Faux)
         	Ferme()
         	
         FIN
         
