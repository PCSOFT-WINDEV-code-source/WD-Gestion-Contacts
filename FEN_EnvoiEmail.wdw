     code : |1-
      // Résumé : Initialise les options pour l'envoi d'email selon ce qui a été coché par l'utilisateur
      // Syntaxe :
      // InitOptionsMail ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      
      Procédure InitOptionsMail()
      
      // Initialisation des variables options
      Email.AccuséRéception = INT_Accusé
      Email.ConfirmationLecture = INT_ConfirmationLecture
      
      SELON SEL_Confidentialité
      	CAS 1 :// Normal
      		Email.Confidentialité = EmailNormal
      	CAS 2 :// Confidentiel
      		Email.Confidentialité = EmailConfidentiel
      	CAS 3 :// Personnel
      		Email.Confidentialité = EmailPersonnel
      	CAS 4 :// Privé
      		Email.Confidentialité = EmailPrivé
      FIN
      
      SELON SEL_Priorité
      	CAS 1 : // haute
      		Email.Priorité = EmailPrioritéHaute
      	CAS 2 : // Normale
      		Email.Priorité = EmailPrioritéNormale
      	CAS 3 : // Basse
      		Email.Priorité = EmailPrioritéBasse
      FIN
