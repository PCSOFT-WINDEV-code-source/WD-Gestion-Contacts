   -
     name : InitDestinataires
     internal_properties : CAAAAAgAAAASvddaaHAZHEYGMSEMVBPslwJnGsAie3EwHq2J2SYImsTcdNjMpu4VXE08UfJxIxXHeCC6pErp5PZvzJYWRPJOjUQNUfok7hpD8Ep1oeK3fTw+p1o1yXzvToHnPF89z4qy9acKNHQks7S1/XVaBTpLTrDfsy52RYbsJ7uuzeL+G/xCEZLcCD+GTbCvbIXyLCAH5FW5w+ZxN0onZ1vw6gI8DASfaA==
     procedure_id : 1749912359518957527
     type_code : 14
     code : |1+
      // Résumé : Initialisation des destinataires pour l'envoi d'email
      // Syntaxe :
      //InitDestinataires (<sListeDestinataires>, <nTypeDest>)
      //
      // Paramètres :
      //	sListeDestinataires : Chaine avec la liste des destinataires séparés par un ";"
      //	nTypeDest : 1 pour destinataires normaux, 2 pour CC, 3 pour CCI
      // Valeur de retour :
      // 	Aucune
      
      Procédure InitDestinataires(sListeDestinataires, nTypeDest)
      
      sDestinataire est une chaine
      nNbDestinataire, i sont des entiers
      ptabDestinataire est un tableau dynamique
      
      // Récupération du nombre de destinataire
      nNbDestinataire = ChaîneOccurrence(sListeDestinataires,";")
      
      //Affectation du nombre de destinataire
      SELON nTypeDest 
      	CAS 1 :
      		// Destinataires principaux
      		Email.NbDestinataire = nNbDestinataire
      	CAS 2 :
      		// Destinataires CC
      		Email.NbCc = nNbDestinataire
      	CAS 3 :
      		// Destinataires CCi
      		Email.NbCci = nNbDestinataire
      FIN
      
      // Allocation du tableau selon le nombre de destinataire
      ptabDestinataire = allouer un tableau dynamique de nNbDestinataire chaînes
      
      // Remplissage du tableau de destinataire
      i = 1
      POUR TOUTE CHAINE sDestinataire DE sListeDestinataires SEPAREE PAR ";"
      	si sDestinataire <> "" alors
      		ptabDestinataire[i] = sDestinataire
      		i++
      	fin
      FIN
      
      //Ici, pas besoin de déclarer la variable j.. le WLangage le gère 
      pour j = 1 a nNbDestinataire
      	// Initialisation de la variable destinataire, cc, ou cci
      	SELON nTypeDest
      		CAS 1 :		
      			// Destinataires principaux
      			Email.Destinataire[j] = ptabDestinataire[j]
      		CAS 2 :
      			// Destinataires CC
      			Email.Cc[j] = ptabDestinataire[j]
      		CAS 3 :
      			// Destinataires CCi
      			Email.Cci[j] = ptabDestinataire[j]
      	FIN
      fin
      
      // On libère le tableau dynamique
      libérer ptabDestinataire
