      PROCEDURE FEN_EnvoiMail( sListeIDContacts )
      
      //Déclaration des variables globales à la fenêtre
      gsEmailExpediteur, gsNomUtilisateur, gsPassWord, gsServeurSMTP, gsAdresseMailContact sont des chaines
      gnOngletPrécédent est un entier
      
      sIDContact est une chaine
      pour TOUTE CHAINE sIDContact DE sListeIDContacts
      	
      	//On se positionne dans le fichier sur le contact dont l'identifiant est sIDContact
      	HLitRecherchePremier(Contact, IDContact, val(sIDContact))
      	//On vérifie si le contact dispose bien d'une adresse email
      	si contact.email <> "" alors
      		//si oui, on l'ajoute à la liste des adresses email destinataire
      		gsAdresseMailContact = gsAdresseMailContact + [";"] + Contact.Email
      	fin
      	
      FIN
   -
     internal_properties : CAAAAAgAAADSxO2yg6vgN+KF+lM0io+5WHFRsg6yu21kfAo8Qe51YooAkSSttMbmzIuM9InpmL+aWQXkG4vlw8GRQxoP/ceYtKfxJt7lrGmTWAhGRnCWrfQzYrfb5Z5nbYdXPsux5s/8yY4dIUM8ep9i4Z222H8/wngKxmApfNQT8NnzyDK+KCAOLgvcZgBWjjeOBJ+1mvkpYj0Sq8j9u5HnZJ4NZ8VW
     code : |1+
      // Traitement automatique des erreurs :   afficher l'erreur puis reprendre la saisie
      //
      
      MaFenêtre..Plan = 1
      
      //Par défaut, on se positionne sur le 1er onglet
      ONG_Informations = 1
      
      gsEmailExpediteur = ChargeParamètre(CST_MEMORISATION_EXPEDITEUR, "")
      gsNomUtilisateur = ChargeParamètre(CST_MEMORISATION_UTILISATEUR, "")
      gsServeurSMTP = ChargeParamètre(CST_MEMORISATION_SERVEUR_SMTP, "")
      
      si fFichierExiste(gsFichierConfig) = faux _ou_ gsEmailExpediteur = "" _OU_ gsServeurSMTP = ""  alors  
      	
      	//On ouvre la fenêtre de paramétrage pour permettre à l'utilisateur de renseigner ces informations	
      	Ouvre(FEN_ParamétrageEmail)	
      	
      	gsServeurSMTP = ChargeParamètre(CST_MEMORISATION_SERVEUR_SMTP, "")
      	SI  gsServeurSMTP = "" ALORS
      		Erreur("Les informations de paramétrage du compte email n'ont pas été correctement saisies. La fenêtre d'envoi d'email va donc se fermer.") 
      		Ferme()
      	FIN
      	
      fin
      
       
      	
      // Récupération des informations de configuration du compte email
      gsNomUtilisateur = ChargeParamètre(CST_MEMORISATION_UTILISATEUR, "")
      gsEmailExpediteur = ChargeParamètre(CST_MEMORISATION_EXPEDITEUR, "")
      gsPassWord = Décrypte(ChargeParamètre(CST_MEMORISATION_MDP, ""),"FD45SR44K")
      gsServeurSMTP = ChargeParamètre(CST_MEMORISATION_SERVEUR_SMTP, "")
      
      // Affichage des options mémorisées dans la fenêtre
      INT_Accusé = chargeparamètre(CST_ACCUSE_RECEPTION, "")
      INT_ConfirmationLecture = ChargeParamètre(CST_CONFIRMATION_LECTURE, "")
      SEL_Confidentialité = ChargeParamètre(CST_CONFIDENTIALITE, "")
      SEL_PRIORITE = ChargeParamètre(CST_PRIORITE, "")
      	
      
      //Si on est arrivé dans cette fenêtre, c'est que l'on dispose de l'adresse email du contact,
      //=> on la met automatiquement dans la liste des destinataires
      //en effet, le bouton "Email" de la fenêtre "FEN_Contacts" est grisé si le contact n'a pas d'email
      SAI_Destinataires = gsAdresseMailContact +";"
