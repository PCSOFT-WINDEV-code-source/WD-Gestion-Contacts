        code : |1-
         bOptionsModifiees est un booleen = faux
         
         //Si on était sur l'onglet des options et que ces options ont été modifiées par l'utilisateur;
         //on lui propose de mémoriser ces nouvelles options
         si gnOngletPrécédent = 3 ALORS
         	
         
         <SI CibleExécution=ApplicationWindows>
         		
         		// Les options ont été modifiées si un des champs du groupe a été modifié
         		bOptionsModifiees = GR_OptionsEmail..Modifié
         	
         <SINON SI CibleExécution=ApplicationLinux32>
         	
         		// Les options ont été modifiées si un des champs du groupe a été modifié
         		bOptionsModifiees = (SEL_Confidentialité..Modifié ou SEL_Priorité..modifie ou INT_Accusé..modifie ou INT_ConfirmationLecture..modifie)
         	
         <FIN>
         
         
         
         		si bOptionsModifiees alors
         		//La fonction "Dialogue" du WLangage permet de proposer aux utilisateurs des messages d'erreur clairs et des réponses claires
         		//Cette fonction dispose d'un assistant qui aide dans ce sens
         		
         		//1 : &Enregistrer
         		//2 : &Ne pas enregistrer
         		SELON Dialogue(<§?184D35840097ED77§>)
         		
         			// &Enregistrer
         			CAS 1 :
         				//Mémorisation des options
         				SauveParamètre(CST_ACCUSE_RECEPTION, INT_Accusé)
         				SauveParamètre(CST_CONFIRMATION_LECTURE, INT_ConfirmationLecture)
         				SauveParamètre(CST_CONFIDENTIALITE, SEL_Confidentialité)
         				SauveParamètre(CST_PRIORITE, SEL_Priorité)
         				
         			// &Ne pas enregistrer
         			CAS 2 :
         				
         		FIN
         		
         		GR_OptionsEmail..Modifié = faux
         		
         	FIn
         FIN
         
         // Change le plan de la fenêtre en fonction du volet de l'onglet
         mafenetre..plan = moimeme
         
         // Mise à jour du nombre de pièces jointes
         SI ONG_Informations = 1 ALORS
         	LIB_NbreFicAttachés = "Vous avez %1 fichier(s) attaché(s)"
         	LIB_NbreFicAttachés = ChaîneConstruit(LIB_NbreFicAttachés,TableOccurrence(TABLE_FichiersAttachés))
         FIN
         
         gnOngletPrécédent = Moimeme
