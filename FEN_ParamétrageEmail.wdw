      -
        internal_properties : CAAAAAgAAABkq+hon0w6pSeA6GRzTYwNplrEBmgXiNC6JQGBgi4YYU4oh95CWJZxphgLOccCttZmwRzngu5A/QZ7i4PwQMu19c4/lZnCuq7NkzToFWrOs3hErVTGUDrFleStAcG6NcX64sA0pKYSHAeXochAjKx9ipXuhzWyxwJvB0WIigf977Yr+Liti/uYy4oMBUBkSTEg
        code : |1-
         
         // Vérification de toutes les données entrées
         
         //On vérifie la validité syntaxique de l'adresse email saisie
         //La fonction WLangage "VérifieExpressionRégulière" dispose d'un assistant pour générer facilement l'expression régulière à vérifier
         //Des expressions régulières courantes sont proposées, telles que l'adresse email par exemple
         si EmailVérifieAdresse(SAI_Expéditeur,emailValideSyntaxeStrict) = emailAdresseSyntaxeIncorrecte ALORS
         	Erreur("Vous devez saisir une adresse e-mail valide.")
         	RepriseSaisie(SAI_Expéditeur)
         FIN
         
         SI SAI_MDP <> SAI_MDPConfirmation ALORS
         	Erreur("La confirmation de votre mot de passe n'est pas valide.")
         	RepriseSaisie(SAI_MDPConfirmation)
         FIN
         
         SI SAI_ServeurSMTP = "" ALORS
         	Erreur("Vous devez saisir l'adresse du serveur SMTP.")
         	RepriseSaisie(SAI_ServeurSMTP)
         FIN
         
         
         // Il n'y a pas eu d'erreur donc on peut valider :
         // enregistrement des informations de configuration du compte e-mail dans le .ini
         SauveParamètre(CST_MEMORISATION_UTILISATEUR, SAI_Email)
         SauveParamètre(CST_MEMORISATION_EXPEDITEUR, SAI_Expéditeur)
         SauveParamètre(CST_MEMORISATION_SERVEUR_SMTP, SAI_ServeurSMTP)
         SauveParamètre(CST_MEMORISATION_MDP, Crypte(SAI_MDP,"FD45SR44K"))
         
         Ferme()
